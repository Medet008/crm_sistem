\n    apiUrl: {\n      type: String,\n      required: true,\n    },\n  },\n  data() {\n    return {\n      // total: 500,\n      limit,\n      url: \"/tags/dragons\",\n    };\n  },\n  computed: {\n    ...mapState({\n      isLoading: (state) => state.feed.isLoading,\n      feed: (state) => state.feed.data,\n      error: (state) => state.feed.error,\n    }),\n    currentPage() {\n      console.log(\"currentPage\", this.$);\n      return Number(this.$router.query.page || \"1\");\n    },\n    baseUrl() {\n      console.log(\"baseUrl\", this.$router);\n      return this.$router.query.baseUrl;\n    },\n    watch: {\n      currentPage() {\n        console.log(\"current page changet\");\n      },\n    },\n  },\n  mounted() {\n    console.log(\"feed\");\n    this.$store.dispatch(actionTypes.getFeed, { apiUrl: this.apiUrl });\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}