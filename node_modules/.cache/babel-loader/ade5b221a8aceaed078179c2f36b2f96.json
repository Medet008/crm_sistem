AAO,IAAIC,OAAJ,CAAY,UAAAC,OAAO,EAAI;AAC5BH,IAAAA,OAAO,CAACI,MAAR,CAAeb,aAAa,CAACC,kBAA7B;AACAL,IAAAA,UAAU,CACPS,aADH,CACiBK,YADjB,EAEGI,IAFH,CAEQ,UAAAC,OAAO,EAAI;AACfN,MAAAA,OAAO,CAACI,MAAR,CAAeb,aAAa,CAACE,oBAA7B,EAAmDa,OAAnD;AACAH,MAAAA,OAAO,CAACG,OAAD,CAAP;AACD,KALH,EAMGC,KANH,CAMS,UAAAC,MAAM,EAAI;AACfR,MAAAA,OAAO,CAACI,MAAR,CACEb,aAAa,CAACG,oBADhB,EAEEc,MAAM,CAACC,QAAP,CAAgBC,IAAhB,CAAqBC,MAFvB;AAID,KAXH;AAYD,GAdM,CAAP;AAeD,CAjBU,CAAb;;AAoBA,eAAe;AACbvB,EAAAA,KAAK,EAALA,KADa;AAEbW,EAAAA,OAAO,EAAPA,OAFa;AAGbF,EAAAA,SAAS,EAATA;AAHa,CAAf","sourcesContent":["import articleApi from '@/api/article'\n\nconst state = {\n  isSubmitting: false,\n  validationErrors: null\n}\n\nexport const mutationTypes = {\n  createArticleStart: '[createArticle] Create article start',\n  createArticleSuccess: '[createArticle] Create article success',\n  createArticleFailure: '[createArticle] Create article failure'\n}\n\nexport const actionTypes = {\n  createArticle: '[createArticle] Create article'\n}\n\nconst mutations = {\n  [mutationTypes.createArticleStart](state) {\n    state.isSubmitting = true\n  },\n  [mutationTypes.createArticleSuccess](state) {\n    state.isSubmitting = false\n  },\n  [mutationTypes.createArticleFailure](state, payload) {\n    state.isSubmitting = false\n    state.validationErrors = payload\n  }\n}\n\nconst actions = {\n  [actionTypes.createArticle](context, {articleInput}) {\n    return new Promise(resolve => {\n      context.commit(mutationTypes.createArticleStart)\n      articleApi\n        .createArticle(articleInput)\n        .then(article => {\n          context.commit(mutationTypes.createArticleSuccess, article)\n          resolve(article)\n        })\n        .catch(result => {\n          context.commit(\n            mutationTypes.createArticleFailure,\n            result.response.data.errors\n          )\n        })\n    })\n  }\n}\n\nexport default {\n  state,\n  actions,\n  mutations\n}\n"]}]}